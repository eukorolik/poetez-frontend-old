{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "eol-last": 0,
    "no-console": ["error"],
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "global-require": 0,
    "object-curly-spacing": ["error", "never"],
    "object-shorthand": 0,
    "comma-dangle": 0,
    "quotes": ["error", "single", {"allowTemplateLiterals": true}],
    "import/no-unresolved": 0,
    "new-cap": 0,
    "no-use-before-define": 0
  }
}
